<div class="container">

    <div class="row col-12 justify-content-center">
        <h3>likes</h3>
        <table class="table">
            <thead>
            <tr>
                <th>user</th>
            </tr>
            </thead>
            <tbody>
            <% if (typeof records !== 'undefined') { %>
            <tr>
                <% _.each(records, function (record) { %>
                <th scope="row">1</th>
                <td><%= record.name %></td>
                <% }) %>
            </tr>
            <% } %>
            </tbody>
        </table>

    </div>

</div>